<?xml version="1.0" encoding="UTF-8" ?>
  <manifest>
  <default sync-j="4" revision="warrior"/>

  <remote fetch="git://git.yoctoproject.org"        name="yocto"/>
  <remote fetch="https://github.com/Freescale"      name="freescale"/>
  <remote fetch="git://git.openembedded.org"        name="oe"/>
  <remote fetch="https://github.com/mirzak"         name="coral"/>
  <remote fetch="https://github.com/kraj"           name="clang"/>

  <project name="poky" remote="yocto" revision="warrior" path="sources/poky"/>
  <project name="meta-freescale" remote="yocto" revision="warrior" path="sources/meta-freescale"/>
  <project name="meta-freescale-3rdparty" remote="freescale" revision="warrior" path="sources/meta-freescale-3rdparty"/>
  <project name="meta-freescale-distro" remote="freescale" revision="warrior" path="sources/meta-freescale-distro"/>
  <project name="meta-openembedded" remote="oe" revision="warrior" path="sources/meta-openembedded"/>
  <project name="meta-clang" remote="clang" revision="warrior" path="sources/meta-clang"/>
  <project name="meta-coral" remote="coral" revision="warrior" path="sources/meta-coral"/>

  <include name="scripts/mender.xml"/>

  </manifest>
